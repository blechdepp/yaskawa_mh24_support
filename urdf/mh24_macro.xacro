<?xml version="1.0" ?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
	<xacro:macro name="motoman_mh24" params="prefix">
		<!-- link list -->
		<link name="${prefix}base_link">
			<visual>
			        <origin rpy="0 0 3.14159"/>
				<geometry>
					<mesh filename="package://motoman_mh24_support/meshes/mh24/visual/base_axis.stl" />
				</geometry>
				<material name="blue">
					<color rgba="0 0 1 1"/>
				</material>
			</visual>
			<collision>
			        <origin rpy="0 0 3.14159"/>
				<geometry>
					<mesh filename="package://motoman_mh24_support/meshes/mh24/collision/base_axis.stl" />
				</geometry>
				<material name="yellow">
					<color rgba="0 1 1 1"/>
				</material>
			</collision>
		</link>
		<link name="${prefix}link_s">
			<visual>
				<geometry>
					<mesh filename="package://motoman_mh24_support/meshes/mh24/visual/s_axis.stl"/>
				</geometry>
				<material name="silver">
					<color rgba="0.8 0.8 0.8 1"/>
				</material>
			</visual>
			<collision>
				<geometry>
					<mesh filename="package://motoman_mh24_support/meshes/mh24/collision/s_axis.stl" />
				</geometry>
				<material name="yellow"/>
			</collision>
		</link>
		<link name="${prefix}link_l">
			<visual>
				<!-- <origin rpy="1.57 3.1416 0" xyz="0 0 0"/> -->
				<geometry>
					<mesh filename="package://motoman_mh24_support/meshes/mh24/visual/l_axis.stl"/>
				</geometry>
				<material name="blue"/>
			</visual>
			<collision>
				<!-- <origin rpy="1.57 3.1416 0" xyz="0 0 0"/> -->
				<geometry>
					<mesh filename="package://motoman_mh24_support/meshes/mh24/collision/l_axis.stl" />
				</geometry>
				<material name="yellow"/>
			</collision>
		</link>
		<link name="${prefix}link_u">
			<visual>
				<!-- <origin rpy="1.57 -3.1415 0" xyz="0 0 0"/> -->
				<geometry>
					<mesh filename="package://motoman_mh24_support/meshes/mh24/visual/u_axis.stl"/>
				</geometry>
				<material name="blue"/>
			</visual>
			<collision>
				<!-- <origin rpy="1.57 -3.1415 0" xyz="0 0 0"/> -->
				<geometry>
					<mesh filename="package://motoman_mh24_support/meshes/mh24/collision/u_axis.stl" />
				</geometry>
				<material name="yellow"/>
			</collision>
		</link>
		<link name="${prefix}link_r">
			<visual>
				<!-- <origin rpy="0 0 3.1416" xyz="0 0 0"/> -->
				<geometry>
					<mesh filename="package://motoman_mh24_support/meshes/mh24/visual/r_axis.stl"/>
				</geometry>
				<material name="silver"/>
			</visual>
			<collision>
				<!-- <origin rpy="0 0 3.1416" xyz="0 0 0"/> -->
				<geometry>
					<mesh filename="package://motoman_mh24_support/meshes/mh24/collision/r_axis.stl" />
				</geometry>
				<material name="yellow"/>
			</collision>
		</link>
		<link name="${prefix}link_b">
			<visual>
				<!-- <origin rpy="-1.57 0 3.1416" xyz="0 0 0"/> -->
				<geometry>
					<mesh filename="package://motoman_mh24_support/meshes/mh24/visual/b_axis.stl"/>
				</geometry>
				<material name="blue"/>
			</visual>
			<collision>
				<!-- <origin rpy="-1.57 0 3.1416" xyz="0 0 0"/> -->
				<geometry>
					<mesh filename="package://motoman_mh24_support/meshes/mh24/collision/b_axis.stl" />
				</geometry>
				<material name="yellow"/>
			</collision>
		</link>
		<link name="${prefix}link_t">
			<visual>
				<geometry>
					<mesh filename="package://motoman_mh24_support/meshes/mh24/visual/t_axis.stl" />
				</geometry>
				<material name="silver"/>
			</visual>
			<collision>
				<geometry>
					<mesh filename="package://motoman_mh24_support/meshes/mh24/collision/t_axis.stl" />
				</geometry>
				<material name="yellow"/>
			</collision>
		</link>
		<!-- end of link list -->
		<!-- joint list -->
		<joint name="${prefix}joint_s" type="revolute">
			<parent link="${prefix}base_link"/>
			<child link="${prefix}link_s"/>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<axis xyz="0 0 1" />
			<limit lower="-2.9671" upper="2.9671" effort="100" velocity="2.96" />
		</joint>
		<joint name="${prefix}joint_l" type="revolute">
			<parent link="${prefix}link_s"/>
			<child link="${prefix}link_l"/>
			<origin xyz="0.150 0 0.505" rpy="0 0 0"/>
			<axis xyz="0 1 0" />
			<limit lower="-1.1345" upper="2.6180" effort="100" velocity="2.96" />
		</joint>
		<joint name="${prefix}joint_u" type="revolute">
			<parent link="${prefix}link_l"/>
			<child link="${prefix}link_u"/>
			<origin xyz="0 0 0.760" rpy="0 0 0"/>
			<axis xyz="0 -1 0" />
			<limit lower="-1.0122" upper="3.2218" effort="100" velocity="2.96" />
		</joint>
		<joint name="${prefix}joint_r" type="revolute">
			<parent link="${prefix}link_u"/>
			<child link="${prefix}link_r"/>
			<origin xyz="0 0 0.200" rpy="0 0 0"/>
			<axis xyz="-1 0 0" />
			<limit lower="-3.3161" upper="3.3161" effort="100" velocity="3.48" />
		</joint>
		<joint name="${prefix}joint_b" type="revolute">
			<parent link="${prefix}link_r"/>
			<child link="${prefix}link_b"/>
			<origin xyz="0.795 0 0" rpy="0 0 0"/>
			<axis xyz="0 -1 0" />
			<limit lower="-2.1817" upper="2.1817" effort="100" velocity="3.48" />
		</joint>
		<joint name="${prefix}joint_t" type="revolute">
			<parent link="${prefix}link_b"/>
			<child link="${prefix}link_t"/>
			<origin xyz="0.100 0 0" rpy="0 0 0"/>
			<axis xyz="-1 0 0" />
			<limit lower="-6.2831" upper="6.2831" effort="100" velocity="6.97" />
		</joint>
		<!-- end of joint list -->
	</xacro:macro>
</robot>

